package com.gao.chapter.one;

public class PrintX {
	public static void main(String[] args) {
		for(int i = 0;i<5;i++) {
			System.out.print("*");
			for(int j = 0;j<8;j++) {
				System.out.print("\b");
				for(int x = 8;x<0;x--) {
					System.out.print(" ");
				}
			}
			System.out.println("*");
		}
	}
}
