		int y = sc.nextInt();
		int z = sc.nextInt();
		int maxVal = 0;
		int midVal = 0;
		int minVal = 0;
		
		minVal = x<y?x:y;
		minVal = minVal<z?minVal:z;
		
	
		maxVal = x<y?y:x;
		maxVal = maxVal<z?z:maxVal;
		
		midVal = x + y + z - maxVal - minVal;

		
		
		System.out.println(minVal + " " + midVal + " " + maxVal);
		
		
		
	
		
