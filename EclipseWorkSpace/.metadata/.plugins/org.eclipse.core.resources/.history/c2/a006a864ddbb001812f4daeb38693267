package com.gao.test;

public class Test18 {
	public static void main(String[] args) {
		Student1 student = new Student1();
		System.out.println(student.id);
	}
}

class Student1{
	public long id;
	static long count = 20157730100L;
	{
		id = count++;
	}
	public Student1() {
		super();
		System.out.println(this.id);
	}
	
}