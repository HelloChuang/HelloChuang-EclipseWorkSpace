package com.gao.test;

import java.util.Scanner;

public class Test5 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
			int i = sc.nextInt();
			System.out.println("====================");
			System.out.println(" 主 "+" 菜 "+" 单 ");
			System.out.println("\t"+"1.注册");
			System.out.println("\t"+"2.登陆");
			System.out.println("\t"+"3.帮助信息");
			System.out.println("请选择： "+i);
			System.out.println("====================");
			 switch(i){
			 case 1 : System.out.println("gundan");
			 case 2: System.out.println("gundan2");
			 case 3 : System.out.println("gundan3");
			 default:System.out.println("无法识别");
			 }
		System.out.println(" gg");
		
	}

}
