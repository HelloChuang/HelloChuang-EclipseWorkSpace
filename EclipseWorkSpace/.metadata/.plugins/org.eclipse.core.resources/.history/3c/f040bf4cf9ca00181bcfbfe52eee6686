package com.gao.test3;

public class Test10 {
	public static void main(String[] args) {
		String s = "931099184";
		String regex = "[1-9]\\d{4,10}";
		System.out.println(s.matches(regex));
		
		String s2 = "www.baidu.com";
		String regex2 = "\\.";
		System.out.println(s2.split(regex2).toString());
		
	}
}
