package com.gao.test;

/**
 * 4.二维数组遍历求和操作：用二重循环求出二维数组b所有元素的和：
 							int[][] b={{11},{21,22},{31,32,33}}
 * @author XX
 *
 */
public class Test14 {
	public static void main(String[] args) {
		int sum = 0;
		int[][] b={{11},{21,22},{31,32,33}};
		for (int[] is : b) {
			for (int i : is) {
				sum+=i;
			}
		}
		System.out.println(sum);
	}
}
