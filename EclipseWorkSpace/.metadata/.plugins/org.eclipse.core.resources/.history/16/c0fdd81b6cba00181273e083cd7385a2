package com.gao.test;

public class Test12 {
	public static void main(String[] args) {
		
		System.out.println(diGui(3));
		printShow(9);
	}
	public static int diGui(int x){
		if(x==1)return 1;
		return x + diGui(x-1);
	}
	public static void printShow(int x){
		for (int i = 1; i < x; i++) {
			for (int j = 1; j <= i ; j++) {
				System.out.println(i+"Ã—" + j+ "=" + i*j +" ");
			}
		}
	}
}
